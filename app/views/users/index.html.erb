


<div class="row">
  <div class="panel">
    <h1 align="center">Admin Page</h1><br>
      <h1>Users List</h1><br>
        <ul>
          <% @users.each do |user| %>
            <li><h4> <%= user.full_name %> </h4></li>
            <%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %><br><br>
          <% end %>
        </ul>
  </div>
</div>
